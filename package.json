{
  "name": "around-me",
  "version": "0.0.0",
  "private": true,
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "postinstall": "npm run vendor && npm run build",
    "build": "./node_modules/gulp/bin/gulp build --prod",
    "vendor": "./node_modules/bower/bin/bower i"
  },
  "engines": {
    "node": "0.10.x"
  },
  "browser": {
    "jquery": "./client/src/vendor/jquery/dist/jquery.js",
    "materialize": "./client/src/vendor/materialize/dist/js/materialize.js",
    "lodash": "./client/src/vendor/lodash/dist/lodash.js",
    "angular": "./client/src/vendor/angular/angular.js",
    "angular-google-maps": "./client/src/vendor/angular-google-maps/dist/angular-google-maps.js"
  },
  "browserify-shim": {
    "jquery": "jquery",
    "materialize": "materialize",
    "lodash": "lodash",
    "angular": "angular",
    "angular-google-maps": {
      "depends": [
        "lodash",
        "angular"
      ]
    }
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "dependencies": {
    "express": "~4.9.0",
    "body-parser": "~1.8.1",
    "cookie-parser": "~1.3.3",
    "morgan": "~1.3.0",
    "serve-favicon": "~2.1.3",
    "debug": "~2.0.0",
    "jade": "~1.6.0",
    "winston": "~0.8.1",
    "nconf": "~0.6.9",
    "mongoose": "~3.8.18",
    "oauth2orize": "~1.0.1",
    "passport": "~0.2.1",
    "passport-http": "~0.2.2",
    "passport-http-bearer": "~1.0.1",
    "passport-oauth2-client-password": "~0.1.2",
    "compression": "~1.2.2",
    "jasmine": "~2.0.1",
    "gulp": "~3.8.10",
    "gulp-clean": "^0.3.1",
    "gulp-jade": "^0.9.0",
    "gulp-plumber": "^0.6.6",
    "gulp-stylus": "^1.3.3",
    "gulp-uglify": "~1.0.2",
    "yargs": "~1.3.3",
    "browserify-shim": "~3.8.2",
    "browserify": "~8.0.2",
    "vinyl-transform": "~1.0.0",
    "gulp-jshint": "~1.9.0",
    "gulp-if": "~1.2.5",
    "gulp-cssmin": "~0.1.6",
    "bower": "~1.3.12"
  }
}
