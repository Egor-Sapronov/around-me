{
  "name": "around-me",
  "version": "0.0.0",
  "private": true,
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "postinstall": "npm run vendor && npm run build",
    "build": "./node_modules/gulp/bin/gulp.js build --prod",
    "vendor": "./node_modules/bower/bin/bower i",
    "test": "node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha test/*"
  },
  "engines": {
    "node": "0.12.x"
  },
  "browser": {
    "jquery": "./client/src/vendor/jquery/dist/jquery.js",
    "materialize": "./client/src/vendor/materialize/dist/js/materialize.js",
    "react": "./client/src/vendor/react/react.js"
  },
  "browserify-shim": {
    "jquery": "jquery",
    "materialize": "materialize",
    "react": "react"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "reactify"
    ]
  },
  "dependencies": {
    "body-parser": "~1.8.1",
    "bower": "~1.3.12",
    "browserify": "~8.0.2",
    "browserify-shim": "~3.8.2",
    "compression": "~1.2.2",
    "cookie-parser": "~1.3.3",
    "debug": "~2.0.0",
    "express": "~4.9.0",
    "gulp": "~3.8.10",
    "gulp-clean": "^0.3.1",
    "gulp-cssmin": "~0.1.6",
    "gulp-if": "~1.2.5",
    "gulp-jade": "^0.9.0",
    "gulp-jshint": "~1.9.0",
    "gulp-plumber": "^0.6.6",
    "gulp-stylus": "^1.3.3",
    "gulp-uglify": "~1.0.2",
    "jade": "~1.6.0",
    "jasmine": "~2.0.1",
    "morgan": "~1.3.0",
    "nconf": "~0.6.9",
    "passport": "~0.2.1",
    "passport-http": "~0.2.2",
    "passport-http-bearer": "~1.0.1",
    "pg": "~4.2.0",
    "pg-hstore": "~2.3.1",
    "reactify": "^1.0.0",
    "sequelize": "^2.0.2",
    "serve-favicon": "~2.1.3",
    "vinyl-transform": "~1.0.0",
    "winston": "~0.8.1",
    "yargs": "~1.3.3"
  },
  "devDependencies": {
    "chai": "~1.10.0",
    "codeclimate-test-reporter": "0.0.4",
    "istanbul": "^0.3.5",
    "mocha": "^2.1.0",
    "supertest": "^0.15.0"
  }
}
